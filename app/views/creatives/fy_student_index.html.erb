
<div class="jumbotron">
    <%@fy_students.each do |fy|%>
    <table class="table table-hover">
        <thead>
            <tr>
                <td>Id</td>
                <td>Student Name</td>
                <td>Year</td>
                <td>Book Name</td>
                <td>Book Count</td>
                <td>Issue Date</td>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td><%=fy.id%></td>
                <td><%=fy.stud_name%></td>
                <td><%=fy.year%></td>
                <td><%=fy.book_name%></td>
                <td><%=fy.book_count%></td>
                <td><%=fy.created_at%></td>
                <td><%=link_to "Edit",:action=>"update_fy_student",:id=>fy.id%></td>
                <td><%=link_to "Delete",:action=>"delete_fy_student",:id=>fy.id%></td>
            </tr>
        </tbody>
    </table>
    <%end%>
    <%=link_to "Add Student",:action => "new_fy_student"%><br>    
    <%=link_to "Back",:action=>"index"%>
</div>    