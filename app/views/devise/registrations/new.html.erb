<div class="container">
      
        		<nav class="navbar navbar-default navbar-fixed-top custom_menu ">
               <div class="navbar-header ">
        		 
              <a class="navbar-brand" href="#">Books Library</a>
              </div>
        	 </nav>
        	
            <div class="jumbotron">
              <h1>Books  Teach Us!!!</h1>
               
                     
              <div class="login_page col-md-3">
              			
                      <h2>Sign Up</h2>
                     <%= form_for(resource, as: resource_name, url: registration_path(resource_name),class:'form_group') do |f| %>
                        <%= devise_error_messages! %>
                        <div class="field">
                         
                          <%= f.email_field :email, autofocus: true,class:'form-control',placeholder:'email' %>
                        </div><br>
        
                      <div class="field">
                       <%if @minimum_password_length%>
                        <em>(<%= @minimum_password_length%>charecters minimum)</em>
                        <%end%><br>
                        <%= f.password_field :password, autocomplete: "off",class:'form-control',placeholder:'password' %>
                      </div><br>
        
                            <div class="field">
                              <%= f.password_field :password_confirmation,autocomplete:"off",class:'form-control',placeholder:'confirm password'%>
                            </div><br>
                        
                     
                      <div class="actions">
                        <%= f.submit "Sign Up" ,class:'form-control',class:'btn btn-success'%>
                      </div><br>
                      
                    <% end %>

                <%=render "devise/shared/links"%>              
                    
                </div>
              </div>
      
               <div class="well custom_footer" >
                      Copyright&nbsp;&copy;&nbsp;
              </div>
              
</div>